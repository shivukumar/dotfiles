# Aliases
alias c=clear
alias g=gvim
alias v=vim
alias scp='scp -r'
alias quark='ssh spatil@quark.ecs.umass.edu'
alias tls='tmux list-sessions'
alias tn='tmux new-session -s'
alias ta='tmux attach-session -t'
alias vlsi='ssh -X spatil@vlsicad2.ecs.umass.edu'


# Environment variables
export QUARK='spatil@quark.ecs.umass.edu'
export VLSI="spatil@vlsicad2.ecs.umass.edu"
